@model TbContact
<html lang="en">

<body>

    <!--section start-->
    <section class="contact-page section-b-space">
        <div class="container">
            <div class="row section-b-space">
                <div class="col-lg-7 map">
                    @*<iframe src="https://www.google.com/maps/embed?pb=!1m14!1m12!1m3!1d1605.811957341231!2d25.45976406005396!3d36.3940974010114!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!5e0!3m2!1sen!2sin!4v1550912388321"
        allowfullscreen></iframe>*@
                    <iframe width="1000" height="400" id="gmap_canvas" src="https://maps.google.com/maps?q=Yukar%C4%B1%20Dudullu,%20Tavuk%C3%A7uyolu%20Cd%20243-233,%2034775%20Dudullu%20Osb/%C3%9Cmraniye/%C4%B0stanbul&t=&z=17&ie=UTF8&iwloc=&output=embed" frameborder="0" scrolling="no" marginheight="0" marginwidth="0"  allowfullscreen></iframe>
                </div>
                <div class="col-lg-5">
                    <div class="contact-right">
                        <ul>
                            <li>
                                <div class="contact-icon">
                                    <img src="~/assets/images/icon/phone.png"
                                         alt="Generic placeholder image">
                                    <h6>Bize Ulaşın</h6>
                                </div>
                                <div class="media-body">
                                    <p>0216 499 11 11</p>
                                    <p>0501 499 11 11</p>
                                    <p>0501 499 66 66</p>
                                    <p>0501 209 62 66</p>
                                </div>
                            </li>
                            <li>
                                <div class="contact-icon">
                                    <i class="fa fa-map-marker" aria-hidden="true"></i>
                                    <h6>Konu</h6>
                                </div>
                                <div class="media-body">
                                    <p>Yukarı Dudullu Mahallesi Tavukçu Yolu Caddesi No: 237/1 Posta34775 Ümraniye / İstanbul</p>
                                    <p>Turkey 34775</p>
                                </div>
                            </li>
                            <li>
                                <div class="contact-icon">
                                    <img src="~/assets/images/icon/email.png"
                                         alt="Generic placeholder image">
                                    <h6>E-Posta</h6>
                                </div>
                                <div class="media-body">
                                    <p>info@zemzemvakif.tc</p>
                                    <p>contact@zemezemvakif.tc</p>
                                </div>
                            </li>
                            <li>
                                <div class="contact-icon">
                                    <i class="fa fa-fax" aria-hidden="true"></i>
                                    <h6>Fax</h6>
                                </div>
                                <div class="media-body">
                                    <p>415 05 05</p>
                                </div>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <form class="theme-form" asp-controller="Contact" asp-action="Save" method="post" role="form">
                        <div class="form-row">
                            <div class="col-md-6">
                                <label for="name">Ad</label>
                                <input type="text" class="form-control" asp-for="FirstName" placeholder="Ad yazın"
                                       required="">
                            </div>
                            <div class="col-md-6">
                                <label for="name">Soyad</label>
                                <input type="text" class="form-control" asp-for="LastName" placeholder="Soyad" required="">
                            </div>
                            <div class="col-md-6">
                                <label for="review">Telefon numarası</label>
                                <input type="number" class="form-control" asp-for="PhoneNumber" placeholder="telefon numarası"
                                       required="">
                                
                            </div>
                            <div class="col-md-6">
                                <label for="email">E-Posta</label>
                                <input type="text" class="form-control" asp-for="Email" placeholder="Email" required="">
                            </div>
                            <div class="col-md-12">
                                <label for="review">Mesaj</label>
                                <textarea class="form-control" placeholder="Mesaj yazın"
                                          asp-for="Message" rows="6" required=""></textarea>
                            </div>
                            <div class="col-md-12">
                                <button class="btn btn-solid"  type="submit">Mesaj Gönder</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </section>
    <!--Section ends-->
  </body>

</html>